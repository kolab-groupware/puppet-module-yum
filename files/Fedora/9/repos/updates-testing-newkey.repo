[updates-testing-newkey]
name=Fedora - $basearch - Test Updates Newkey
failovermethod=priority
baseurl=http://download.fedoraproject.org/pub/fedora/linux/updates/testing/9/$basearch.newkey/
mirrorlist=http://mirrors.fedoraproject.org/mirrorlist?repo=updates-testing-f9.newkey&arch=$basearch
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-8-and-9-$basearch

