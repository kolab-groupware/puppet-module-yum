# Managed with puppet
[livna-testing]
name=Livna for Fedora - Testing
baseurl=http://www.kanarip.com/livna/testing/7/$basearch/
        http://rpm.livna.org/fedora/testing/7/$basearch/
        http://livna.cat.pdx.edu/fedora/testing/7/$basearch/
        http://wftp.tu-chemnitz.de/pub/linux/livna/fedora/testing/7/$basearch/
        http://ftp-stud.fht-esslingen.de/pub/Mirrors/rpm.livna.org/fedora/testing/7/$basearch/
        http://mirror.atrpms.net/livna/fedora/testing/7/$basearch/
        ftp://mirrors.tummy.com/pub/rpm.livna.org/fedora/testing/7/$basearch/
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-7-livna

[livna-testing-debug]
name=Livna for Fedora - Testing - Debug
baseurl=http://www.kanarip.com/livna/testing/7/$basearch/debug/
enabled=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-7-livna
gpgcheck=1

[livna-testing-source]
name=Livna for Fedora - Testing - Source
baseurl=http://www.kanarip.com/livna/testing/7/SRPMS/
enabled=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-7-livna
gpgcheck=1
