# Managed with puppet
[livna]
name=Livna for Fedora - Base
baseurl=http://www.kanarip.com/livna/7/$basearch/
        http://rpm.livna.org/fedora/7/$basearch/
        http://livna.cat.pdx.edu/fedora/7/$basearch/
        http://wftp.tu-chemnitz.de/pub/linux/livna/fedora/7/$basearch/
        http://ftp-stud.fht-esslingen.de/pub/Mirrors/rpm.livna.org/fedora/7/$basearch/
        http://mirror.atrpms.net/livna/fedora/7/$basearch/
        ftp://mirrors.tummy.com/pub/rpm.livna.org/fedora/7/$basearch/
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-7-livna

[livna-debuginfo]
name=Livna for Fedora - Debug
baseurl=http://www.kanarip.com/livna/7/$basearch/debug/
        http://rpm.livna.org/fedora/7/$basearch/debug/
        http://livna.cat.pdx.edu/fedora/7/$basearch/debug/
        http://wftp.tu-chemnitz.de/pub/linux/livna/fedora/7/$basearch/debug/
        http://ftp-stud.fht-esslingen.de/pub/Mirrors/rpm.livna.org/fedora/7/$basearch/debug/
        http://mirror.atrpms.net/livna/fedora/7/$basearch/debug/
        ftp://mirrors.tummy.com/pub/rpm.livna.org/fedora/7/$basearch/debug/
enabled=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-7-livna
gpgcheck=1

