# Managed with puppet
# ./distr/files/Fedora/12/etc/yum/repos.d/livna.repo
[livna]
name=Livna for Fedora - Base
baseurl=http://rpm.livna.org/repo/12/$basearch/
enabled=1
gpgcheck=0

[livna-debuginfo]
name=Livna for Fedora - Debug
baseurl=http://rpm.livna.org/repo/12/$basearch/debug/
enabled=0
gpgcheck=0

[livna-source]
name=Livna for Fedora - Source
baseurl=http://rpm.livna.org/repo/12/SRPMS/
enabled=0
gpgcheck=0
