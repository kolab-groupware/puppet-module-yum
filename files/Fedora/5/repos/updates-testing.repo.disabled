# Managed with puppet
[updates-testing]
name=Fedora 5 - Test Updates
baseurl=http://download.fedoraproject.org/pub/fedora/linux/core/updates/testing/5/$basearch/
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-5-fedora-test

[updates-testing-debuginfo]
name=Fedora 5 - Test Updates Debug
baseurl=http://download.fedoraproject.org/pub/fedora/linux/core/updates/testing/5/$basearch/debug/
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-5-fedora-test

[updates-testing-source]
name=Fedora 5 - Test Updates Source
baseurl=http://download.fedoraproject.org/pub/fedora/linux/core/updates/testing/5/SRPMS/
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-5-fedora-test
