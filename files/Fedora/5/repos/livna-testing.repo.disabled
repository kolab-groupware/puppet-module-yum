# Managed with puppet
[livna-testing]
name=Livna for Fedora 5 - Testing
baseurl=http://www.kanarip.com/livna/testing/5/$basearch/
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-5-livna

[livna-testing-debug]
name=Livna for Fedora 5 - Testing - Debug
baseurl=http://www.kanarip.com/livna/testing/5/$basearch/debug/
enabled=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-5-livna
gpgcheck=1

[livna-testing-source]
name=Livna for Fedora 5 - Testing - Source
baseurl=http://www.kanarip.com/livna/testing/5/SRPMS/
enabled=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-5-livna
gpgcheck=1
