# Please note that the mirrorlists URLs are only temporary and will be changed soon.
# The debuginfo and source repos will also be switching from baseurl to a
# mirror list.

[rpmfusion-nonfree-updates-testing]
name=RPM Fusion for Fedora - Non-Free - Test Updates
baseurl=http://download1.rpmfusion.org/nonfree/fedora/updates/testing/8/$basearch/
mirrorlist=http://download1.rpmfusion.org/mirrorlist?repo=nonfree-testing-8&arch=$basearch
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-rpmfusion-nonfree-fedora

[rpmfusion-nonfree-updates-testing-debuginfo]
name=RPM Fusion for Fedora - Non-Free - Test Updates Debug
baseurl=http://download1.rpmfusion.org/nonfree/fedora/updates/testing/8/$basearch/debug/
mirrorlist=http://mirrors.rpmfusion.org/mirrorlist?repo=nonfree-testing-debug-8&arch=$basearch
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-rpmfusion-nonfree-fedora

[rpmfusion-nonfree-updates-testing-source]
name=RPM Fusion for Fedora - Non-Free - Test Updates Source
baseurl=http://download1.rpmfusion.org/nonfree/fedora/updates/testing/8/SRPMS/
mirrorlist=http://mirrors.rpmfusion.org/mirrorlist?repo=nonfree-testing-source-8&arch=$basearch
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-rpmfusion-nonfree-fedora

