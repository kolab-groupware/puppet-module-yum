# Managed with puppet
# ./distr/files/Fedora/10/etc/yum/repos.d/updates-testing.repo
[updates-testing]
name=Fedora - $basearch - Test Updates
baseurl=http://download.fedoraproject.org/pub/fedora/linux/updates/testing/10/$basearch/
mirrorlist=http://mirrors.fedoraproject.org/mirrorlist?repo=updates-testing-f10&arch=$basearch
enabled=1
gpgcheck=0

[updates-testing-debuginfo]
name=Fedora - $basearch - Test Updates Debug
baseurl=http://download.fedoraproject.org/pub/fedora/linux/updates/testing/10/$basearch/debug/
mirrorlist=http://mirrors.fedoraproject.org/mirrorlist?repo=updates-testing-debug-f10&arch=$basearch
enabled=0
gpgcheck=0

[updates-testing-source]
name=Fedora - Test Updates Source
baseurl=http://download.fedoraproject.org/pub/fedora/linux/core/updates/testing/10/SRPMS/
mirrorlist=http://mirrors.fedoraproject.org/mirrorlist?repo=updates-testing-source-f10&arch=$basearch
enabled=0
gpgcheck=0
