# Managed with puppet
[updates-testing]
name = Fedora 21 - $basearch - Updates
baseurl = http://download.fedoraproject.org/pub/fedora/linux/updates-testing/21/$basearch/
mirrorlist = http://mirrors.fedoraproject.org/mirrorlist?repo=updates-testing-f21&arch=$basearch
enabled = 0
gpgcheck = 1
gpgkey = file:///etc/pki/rpm-gpg/RPM-GPG-KEY-updates-testing

[updates-testing-debuginfo]
name = Fedora - $basearch - Updates - Debug
baseurl = http://download.fedoraproject.org/pub/fedora/linux/updates-testing/21/$basearch/debug/
mirrorlist = http://mirrors.fedoraproject.org/mirrorlist?repo=updates-testing-debug-f21&arch=$basearch
enabled = 0
gpgcheck = 1
gpgkey = file:///etc/pki/rpm-gpg/RPM-GPG-KEY-updates-testing

[updates-testing-source]
name = Fedora - Updates Source
baseurl = http://download.fedoraproject.org/pub/fedora/linux/updates-testing/21/SRPMS/
mirrorlist = http://mirrors.fedoraproject.org/mirrorlist?repo=updates-testing-source-f21&arch=$basearch
enabled = 0
gpgcheck = 1
gpgkey = file:///etc/pki/rpm-gpg/RPM-GPG-KEY-updates-testing
