# Managed with puppet
[livna-testing]
name=Livna for Fedora 6 - Testing
baseurl=http://www.kanarip.com/livna/testing/6/$basearch/
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-6-livna

[livna-testing-debug]
name=Livna for Fedora 6 - Testing - Debug
baseurl=http://www.kanarip.com/livna/testing/6/$basearch/debug/
enabled=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-6-livna
gpgcheck=1

[livna-testing-source]
name=Livna for Fedora 6 - Testing - Source
baseurl=http://www.kanarip.com/livna/testing/6/SRPMS/
enabled=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-6-livna
gpgcheck=1
