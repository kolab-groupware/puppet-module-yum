[updates]
name = CentOS Updates
mirrorlist = http://mirrorlist.centos.org/?release=6&arch=$basearch&repo=updates
enabled = 1
gpgcheck = 1
gpgkey = file:///etc/pki/rpm-gpg/RPM-GPG-KEY-updates

[updates-debuginfo]
name = CentOS 6 Release - Debug
baseurl = http://debuginfo.centos.org/$releasever/$basearch/
enabled = 0
gpgcheck = 1
gpgkey = file:///etc/pki/rpm-gpg/RPM-GPG-KEY-updates
