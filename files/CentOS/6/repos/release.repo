[release]
name = CentOS Base
mirrorlist = http://mirrorlist.centos.org/?release=6&arch=$basearch&repo=os
enabled = 1
gpgcheck = 1
gpgkey = file:///etc/pki/rpm-gpg/RPM-GPG-KEY-release

[release-debuginfo]
name = CentOS 6 Release - Debug
baseurl = http://debuginfo.centos.org/$releasever/$basearch/
enabled = 0
gpgcheck = 1
gpgkey = file:///etc/pki/rpm-gpg/RPM-GPG-KEY-release
